(FoodContext);\n\n  //use this then we can map out the cart Items using FoodinCart.id\n  return (\n    <React.Fragment>\n      {/* map out Cart Items when Cart opens*/}\n      {ctx.cartModalOpen && <CartModal></CartModal>}\n    </React.Fragment>\n  );\n};\n\nexport default Cart;\n",["77","78"],"/Users/yanyixue/Desktop/HTML, CSS, JS/Learning react/Food-Order-App/src/components/Header+Cart/HeaderCart/HeaderCart.js",["79","80","81"],[],"/Users/yanyixue/Desktop/HTML, CSS, JS/Learning react/Food-Order-App/src/components/Header+Cart/CartToggleContext/CartToggleContext.js",[],[],"/Users/yanyixue/Desktop/HTML, CSS, JS/Learning react/Food-Order-App/src/components/FoodPortion/FoodContext/FoodContext.js",["82"],[],"import React, { useState , useEffect } from \"react\";\n\n// to retrieve food & quantity to be added from FoodItem component to Cart component\nconst FoodContext = React.createContext({\n  updateFoodList: (FoodId, QuantityToAdd) => {},\n  FoodList: { id: 0 },\n});\n\nexport const FoodContextProvider = (props) => {\n  const [FoodList, setFoodList] = useState([\n    {\n      food: \"Sushi\",\n      details: \"Finest fish and veggies\",\n      id: \"A1\",\n      Quantity: 0,\n      Price: 22.99,\n    },\n    {\n      food: \"Schnitzel\",\n      details: \"A german speciality!\",\n      id: \"A2\",\n      Quantity: 0,\n      Price: 16.5,\n    },\n    {\n      food: \"Barbecue Burger\",\n      details: \"American, raw, meaty\",\n      id: \"A3\",\n      Quantity: 0,\n      Price: 12.99,\n    },\n    {\n      food: \"Green Bowl\",\n      details: \"Healthy...and green...\",\n      id: \"A4\",\n      Quantity: 0,\n      Price: 18.99,\n    },\n  ]);\n\n  // function to update FoodListstate\n  const updateFoodList = (FoodId, QuantityToAdd) => {\n    setFoodList((prev) => {\n      // add quantity to be added to cart\n      // iterate through object to check whether key match id and then update the value\n      for (var i in prev) {\n        if (prev[i].id === FoodId && prev[i].Quantity >= 0) {\n          // if quantity = 0, cannot decrease to -1\n          if (QuantityToAdd === -1 && prev[i].Quantity === 0) {\n            break;\n          } \n          // update quantity of a certain Food in the Cart base on Id\n          prev[i].Quantity += +QuantityToAdd;\n        }\n      }\n      return prev;\n    });\n  };\n\n\n  return (\n    <FoodContext.Provider\n      value={{\n        updateFoodList: updateFoodList,\n        FoodList: FoodList,\n      }}\n    >\n      {props.children}\n    </FoodContext.Provider>\n  );\n};\n\nexport default FoodContext;\n","/Users/yanyixue/Desktop/HTML, CSS, JS/Learning react/Food-Order-App/src/components/FoodPortion/FoodItem/FoodItem.js",[],[],"/Users/yanyixue/Desktop/HTML, CSS, JS/Learning react/Food-Order-App/src/components/Header+Cart/CartItem/CartItem.js",[],[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"83","replacedBy":"87"},{"ruleId":"85","replacedBy":"88"},{"ruleId":"89","severity":1,"message":"90","line":9,"column":9,"nodeType":"91","messageId":"92","endLine":9,"endColumn":16},{"ruleId":"83","replacedBy":"93"},{"ruleId":"85","replacedBy":"94"},{"ruleId":"89","severity":1,"message":"95","line":1,"column":41,"nodeType":"91","messageId":"92","endLine":1,"endColumn":50},{"ruleId":"89","severity":1,"message":"96","line":14,"column":10,"nodeType":"91","messageId":"92","endLine":14,"endColumn":23},{"ruleId":"89","severity":1,"message":"97","line":14,"column":25,"nodeType":"91","messageId":"92","endLine":14,"endColumn":41},{"ruleId":"89","severity":1,"message":"95","line":1,"column":28,"nodeType":"91","messageId":"92","endLine":1,"endColumn":37},"no-native-reassign",["98"],"no-negated-in-lhs",["99"],["98"],["99"],"no-unused-vars","'foodctx' is assigned a value but never used.","Identifier","unusedVar",["98"],["99"],"'useEffect' is defined but never used.","'headerCartQty' is assigned a value but never used.","'setHeaderCartQty' is assigned a value but never used.","no-glob